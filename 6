import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Вводим количество элементов
        System.out.print("Введите количество элементов: ");
        int n = scanner.nextInt();

        // Создаем массив для хранения элементов
        int[] sequence = new int[n];

        // Вводим элементы и сохраняем их в массиве
        System.out.println("Введите элементы последовательности (в невозрастающем порядке):");
        for (int i = 0; i < n; i++) {
            sequence[i] = scanner.nextInt();
        }

        // Вводим число, которое нужно найти
        System.out.print("Введите число для поиска: ");
        int target = scanner.nextInt();

        // Ищем позицию числа в последовательности
        int position = findPosition(sequence, target);

        // Выводим результат
        System.out.println("Позиция числа " + target + " в последовательности: " + position);
    }

    // Метод для поиска позиции числа в невозрастающей последовательности
    public static int findPosition(int[] sequence, int target) {
        for (int i = 0; i < sequence.length; i++) {
            if (sequence[i] == target) {
                return i + 1; // Возвращаем позицию (плюс 1, так как индексация начинается с 0)
            } else if (sequence[i] < target) {
                return -1; // Если число не найдено, возвращаем -1
            }
        }
        return -1; // Если число не найдено, возвращаем -1
    }
}
