import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Вводим число n
        System.out.print("Введите число n: ");
        int n = scanner.nextInt();

        // Создаем массив для хранения n чисел
        int[] numbers = new int[n];

        // Вводим n чисел и сохраняем их в массиве
        for (int i = 0; i < n; i++) {
            System.out.print("Введите число " + (i+1) + ": ");
            numbers[i] = scanner.nextInt();
        }

        // Вводим число, на которое нужно умножить
        System.out.print("Введите число для умножения: ");
        int multiplier = scanner.nextInt();

        // Умножаем числа и выводим результат построчно
        for (int i = 0; i < n; i++) {
            int result = numbers[i] * multiplier;
            System.out.println("Результат: " + result);
        }
    }
}
