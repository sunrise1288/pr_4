import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Вводим количество элементов
        System.out.print("Введите количество элементов: ");
        int n = scanner.nextInt();

        // Создаем массив для хранения элементов
        int[] numbers = new int[n];

        // Вводим элементы
        System.out.println("Введите элементы массива:");
        for (int i = 0; i < n; i++) {
            numbers[i] = scanner.nextInt();
        }

        // Вводим значения a и b
        System.out.print("Введите a: ");
        int a = scanner.nextInt();
        System.out.print("Введите b: ");
        int b = scanner.nextInt();

        // Проверяем, что a и b не выходят за границы размеров массива
        if (a >= 1 && a <= n && b >= 1 && b <= n) {
            // Меняем местами значения a-ого и b-ого элементов
            int temp = numbers[a - 1];
            numbers[a - 1] = numbers[b - 1];
            numbers[b - 1] = temp;

            // Выводим массив построчно
            System.out.println("Измененный массив:");
            for (int i = 0; i < n; i++) {
                System.out.println(numbers[i]);
            }
        } else {
            System.out.println("Значения a и b должны быть в пределах от 1 до " + n);
        }
    }
}
